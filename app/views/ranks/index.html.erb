<%= @rank %>
<%= form_with url: searches_path, method: :post, local: true do |f| %>
  <%= f.hidden_field :rank, value: params[:rank] %>
  <%= f.check_box :search_query_01 %>
  <%= f.label :射線管理 %>
  <%= f.check_box :search_query_02 %>
  <%= f.label :撃ち合い %>
  <%= f.check_box :search_query_03 %>
  <%= f.label :立ち回り %>
  <%= f.check_box :search_query_04 %>
  <%= f.label :コーチング動画 %>
  <%= f.submit "検索する" %>
<% end %>